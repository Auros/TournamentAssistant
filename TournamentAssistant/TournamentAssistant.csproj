  <ItemGroup>
    <EmbeddedResource Include="manifest.json" />
    <EmbeddedResource Include="Views\ip-connection-view.bsml" />
    <EmbeddedResource Include="Views\ongoing-game-list-view.bsml" />
    <EmbeddedResource Include="Views\patch-notes-view.bsml" />
    <EmbeddedResource Include="Views\player-list-view.bsml" />
