<bg size-delta-y="0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd">
  <horizontal bg="panel-top" pad-left="10" pad-right="10" horizontal-fit="PreferredSize" anchor-min-x="-0.1" anchor-max-x="-0.1" anchor-min-y="0.95" anchor-max-y="0.95">
    <text text="Ongoing Games" align="Left" />
  </horizontal>
  <page-button direction="Up" event-click="game-list#PageUp" anchor-min-y="0.95" anchor-max-y="0.95" />
  <list id="game-list" select-cell="game-clicked" expand-cell="true" list-style="Simple" />
  <page-button direction="Down" event-click="game-list#PageUp" anchor-min-y="0.05" />
</bg>